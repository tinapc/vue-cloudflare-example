{
  "name": "vue-cloudflare-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "deploy": "npm run build && wrangler deploy",
    "preview": "cross-env VITE_NODE_ENV=production node server"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.13.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "node-html-parser": "^7.0.1",
    "sirv": "^3.0.2",
    "vite-ssr-vue": "^1.1.5",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "wrangler": "^4.37.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^10.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
